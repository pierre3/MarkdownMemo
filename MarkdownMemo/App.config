<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="DefaultStyleSheet" value="style.css"/>
  </appSettings>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <remove name="Default"/>
        <add name="LogFile" type="System.Diagnostics.TextWriterTraceListener" initializeData="%LOCALAPPDATA%\MarkdownMemo\Trace.Log"/>
      </listeners>
    </trace>
  </system.diagnostics>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
